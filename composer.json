{
    "name": "favinblockchain/acl-manager",
    "type": "laravel-package",
    "description": "This package provides access control list and user manager in laravel apps.",
    "license": "MIT",
    "homepage": "https://github.com/favinblockchain/acl-manager",
    "authors": [
        {
            "name": "Sina Rajabpour",
            "email": "sina.rajabpour@outlook.com"
        }
    ],
    "require": {
        "php": "7.1.*|7.2.*|7.3.*|7.4.*|8.*",
        "laravel/framework": "7.*|8.*",
        "santigarcor/laratrust": "6.*",
        "favinblockchain/alpha-helper": "1.*",
        "favinblockchain/log-manager": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Favinblockchain\\AclManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
